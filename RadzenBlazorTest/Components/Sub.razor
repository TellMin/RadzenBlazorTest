<RadzenLabel Text="@subName" />
<RadzenImage Path="@IconPath" Style="height:64px;width:64px"/>

@code {
    [Parameter]
    public string subName { get; set; } = string.Empty;

    [Parameter]
    public string SubId { get; set; } = string.Empty;

    private string IconPath
    {
        get => $"/sub/S3_Weapon_Sub_{SubId}.png";
    }
}
